@model MvcApplication1.Models.Item

@{
    ViewBag.Title = "Delete";
}
@section headermenu
    { 

             @Html.ActionLink("Back", "Menu", "User",new{id=Model.ExpertSystemId}, new {@class="MainMenuPart" })
}
<h2>Delete Item</h2>

<h3>Are you shure that you want to delete this object?</h3>
<fieldset>
    <legend>Item</legend>

    <div class="display-label">
         @Html.DisplayNameFor(model => model.Name)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Name)
    </div>
    @foreach (var val in Model.Note)
    {  <div class="display-label">
        @val.Val.Property.Name
        </div>
    <div class="display-field">
        @Html.DisplayName(val.Val.Mean)
    </div>
    }
</fieldset>
@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    <p>
        <input type="submit" value="Delete" /> 
    </p>
}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}